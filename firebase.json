{
  "hosting": {
    "public": "_site",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/artikel/backbone-chaplin-und-react/",
        "destination": "/artikel/backbone-chaplin-react/",
        "type": 301
      },
      {
        "source": "/posts/wie-wahlt-man-die-beste-flux-implementierung",
        "destination": "/artikel/flux-implementierung-auswaehlen/",
        "type": 301
      },
      {
        "source": "/posts/die-flux-architektur-und-react",
        "destination": "/artikel/react-flux-architektur/",
        "type": 301
      },
      {
        "source": "/artikel/reactjs-typescript/",
        "destination": "/artikel/react-typescript/",
        "type": 301
      },
      {
        "source": "/artikel/virtuelles-dom-mit-react-js/",
        "destination": "/artikel/vdom-react/",
        "type": 301
      },
      {
        "source": "/posts/virtuelles-dom-mit-react-js",
        "destination": "/artikel/vdom-react/",
        "type": 301
      },
      {
        "source": "/posts/javascript-module-eine-einleitung",
        "destination": "/artikel/javascript-module-einleitung/",
        "type": 301
      },
      {
        "source": "/posts/react-tutorial",
        "destination": "/artikel/react-tutorial-deutsch/",
        "type": 301
      },
      {
        "source": "/posts/react-anwendungen-mit-webpack-entwickeln/",
        "destination": "/artikel/react-webpack",
        "type": 301
      },
      {
        "source": "/posts/react-tutorial",
        "destination": "/artikel/react-und-babel-mit-es6-und-es7/",
        "type": 301
      },
      {
        "source": "/posts/:id*",
        "destination": "/artikel/:id/",
        "type": 301
      },
      {
        "source": "/users",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/users/*",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/jobs",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/jobs/*",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/links",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/links/*",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/ueber-uns/",
        "destination": "/team/",
        "type": 301
      }
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|svg|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=604800"
          }
        ]
      },
      {
        "source": "**/feed.xml",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/rss+xml"
          }
        ]
      }
    ]
  }
}
